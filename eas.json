{
  "cli": {
    "version": ">= 15.0.13",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "env": {
        "APP_ENV": "development"
      },
      "autoIncrement": true,
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "env": {
        "APP_ENV": "preview"
      },
      "distribution": "internal",
      "autoIncrement": true,
      "channel": "preview"
    },
    "production": {
      "env": {
        "APP_ENV": "production"
      },
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
